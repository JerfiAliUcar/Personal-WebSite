@model List<Experience>

<section class="resume-section" id="experience">

    <div class="resume-section-content">
        <h2 class="mb-5">PROJELERİM</h2>
        @foreach (Experience item in Model)
        {
            <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
                <div class="flex-grow-1">
                    <h3 class="mb-0">@item.Title</h3>
                    <div class="subheading mb-3">@item.SubTitle</div>
                    <p>@item.Description</p>
                    <i class="fa-solid fa-link"></i>  <a href="@item.Link">Proje Linki</a>
                </div>
                <div class="flex-shrink-0"><span class="text-primary">@item.Tarih</span></div>
            </div>
        }
    </div>
</section>
<hr class="m-0" />