@model List<Education>


<section class="resume-section" id="education">
    <div class="resume-section-content">
        <h2 class="mb-5">EĞİTİM BİLGİLERİM</h2>

        @foreach (Education item in Model)
        {
            <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
                <div class="flex-grow-1">
                    <h3 class="mb-0">@item.Title</h3>
                    <div class="subheading mb-3">@item.SubTitle1</div>
                    <div>@item.SubTitle2</div>
                    <p>GPA: @item.GNO</p>
                </div>
                <div class="flex-shrink-0"><span class="text-primary">@item.Tarih</span></div>
            </div>
        }
    </div>
</section>
<hr class="m-0" />