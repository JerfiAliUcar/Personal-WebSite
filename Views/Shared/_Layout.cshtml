<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Resume - Start Bootstrap Theme</title>
    <link rel="icon" type="image/x-icon" href="~/assets/img/profile.jpg" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v5.15.1/js/all.js" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/997a8bee10.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/masonry-layout@4.2.2/dist/masonry.pkgd.min.js" integrity="sha384-GNFwBvfVxBkLMJpYMOABq3c+d3KnQxudP/mGPkzpZSTYykLBNsZEnG2D9G/X/+7D" crossorigin="anonymous" async></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:500,700" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Muli:400,400i,800,800i" rel="stylesheet" type="text/css" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/css/styles.css" rel="stylesheet" />
</head>
<body id="page-top">
    <!-- Navigation-->
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
        <a class="navbar-brand js-scroll-trigger" href="#page-top">
            <span class="d-block d-lg-none">Jerfi Ali Uçar</span>
            <span class="d-none d-lg-block"><img class="img-fluid img-profile rounded-square mx-auto mb-2" src="~/assets/img/JAU.jpeg" alt="" /></span>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Home" asp-action="Index">HAKKIMDA</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Home" asp-action="Experience">PROJELER</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Personal" asp-action="Education">EĞİTİM</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Personal" asp-action="Skills">BECERİLER</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Personal" asp-action="Hobbies">İLGİ ALANLARI / HOBİLER</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Personal" asp-action="Certification">SERTİFİKALAR</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-controller="Contact" asp-action="ContactMe">İLETİŞİM</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" asp-area="Admin" asp-controller="About" asp-action="Index">ADMIN PANEL</a></li>
                

                @if (Context.User.Identity.IsAuthenticated)
                {                    
                    <li class="nav-item"><a class="nav-link" asp-controller="Auth" asp-action="Logout">Logout</a></li>

                }
                else
                {
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Auth" asp-action="Login">Login</a>
                    </li>
                }
            </ul>



        </div>
    </nav>
    <!-- Page Content-->

    <div class="container-fluid p-0">
        <!-- About-->
        <!-- Experience-->
        <!-- Education-->
        <!-- Skills-->
        <!-- Interests-->
        <!-- Awards-->
        @RenderBody()

    </div>
    <!-- Bootstrap core JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/js/scripts.js"></script>
</body>
</html>
